---
import type { Icon } from "@/lib/types";

type Props = Icon;

const {
  width = "20",
  height = "20",
  color = "currentColor",
  strokeWidth = "1.5",
  size,
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  width={size ? size : width}
  height={size ? size : height}
  color={color}
  fill="none"
>
  <path
    d="M3 21V3H21"
    stroke="currentColor"
    stroke-width={strokeWidth}
    stroke-linecap="round"
    stroke-linejoin="round"></path>
  <path
    d="M7 14L11 10L15 14L19 8"
    stroke="currentColor"
    stroke-width={strokeWidth}
    stroke-linecap="round"
    stroke-linejoin="round"></path>
  <circle cx="7" cy="14" r="1" fill="currentColor"></circle>
  <circle cx="11" cy="10" r="1" fill="currentColor"></circle>
  <circle cx="15" cy="14" r="1" fill="currentColor"></circle>
  <circle cx="19" cy="8" r="1" fill="currentColor"></circle>
</svg>
