---
import type { Icon } from "@/lib/types";

type Props = Icon;

const {
  width = "20",
  height = "20",
  color = "currentColor",
  strokeWidth = "1.5",
  size,
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  width={size ? size : width}
  height={size ? size : height}
  color={color}
  fill="none"
>
  <path
    d="M3 21V3H21"
    stroke="currentColor"
    stroke-width={strokeWidth}
    stroke-linecap="round"
    stroke-linejoin="round"></path>
  <rect
    x="6"
    y="12"
    width="3"
    height="9"
    stroke="currentColor"
    stroke-width={strokeWidth}
    fill="none"></rect>
  <rect
    x="10.5"
    y="8"
    width="3"
    height="13"
    stroke="currentColor"
    stroke-width={strokeWidth}
    fill="none"></rect>
  <rect
    x="15"
    y="15"
    width="3"
    height="6"
    stroke="currentColor"
    stroke-width={strokeWidth}
    fill="none"></rect>
</svg>
